<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="480.0" prefWidth="854.0" xmlns="http://javafx.com/javafx/17.0.6" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.ufrn.imd.controller.ScoreBoardController">
   <center>
      <VBox alignment="TOP_CENTER" prefHeight="415.0" prefWidth="600.0">
          <TableView fx:id="scoreTable" prefHeight="400.0" prefWidth="834.0">
              <columns>
                  <TableColumn fx:id="nameColumn" prefWidth="427.0" text="Name" />
                  <TableColumn fx:id="scoreColumn" prefWidth="427.0" text="Score" />
              </columns>
            <columnResizePolicy>
               <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
          </TableView>
         <Button alignment="CENTER" mnemonicParsing="false" onAction="#handleBackButtonAction" prefWidth="64.8" text="Back">
            <VBox.margin>
               <Insets top="30.0" />
            </VBox.margin></Button>
      </VBox>
   </center>
</BorderPane>
